{
  "name": "holliston-real-estate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "etl": "DEBUG=etl:* node --trace-warnings --unhandled-rejections=strict ./etl/src/main.js 2>&1 | tee ./etl/logs/etl-$(date +%s%N).log",
    "test": "mocha **/test/*.spec.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "kill": "pgrep -f \"vue-cli-service\" | xargs kill"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.5.0",
    "@aws-sdk/client-s3": "^3.5.0",
    "@aws-sdk/client-textract": "^3.5.0",
    "@aws-sdk/lib-storage": "^3.5.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "cheerio": "^1.0.0-rc.5",
    "datatables.net": "^1.10.23",
    "datatables.net-bs4": "^1.10.23",
    "debug": "^4.3.1",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "street-suffix": "^1.0.1",
    "url-to-s3": "^1.0.0",
    "uuid": "^8.3.2",
    "yargs": "^16.2.0",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-vue": "^7.0.0-0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es2020": true,
      "node": true,
      "mocha": true
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-essential",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "strict": "error",
      "semi": "error",
      "prettier/prettier": "error"
    },
    "ignorePatterns": [
      "node_modules/"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "quoteProps": "consistent"
  }
}
