{
  "name": "holliston-real-estate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "etl": "DEBUG=etl:* node --trace-warnings --unhandled-rejections=strict ./src/etl/main.js 2>&1 | tee etl-$(date +%s%N).log",
    "test": "mocha"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.5.0",
    "@aws-sdk/client-s3": "^3.5.0",
    "@aws-sdk/client-textract": "^3.5.0",
    "@aws-sdk/lib-storage": "^3.5.0",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "debug": "^4.3.1",
    "street-suffix": "^1.0.1",
    "uuid": "^8.3.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es2020": true,
      "node": true,
      "mocha": true
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "rules": {
      "strict": "error",
      "semi": "error",
      "prettier/prettier": "error"
    },
    "ignorePatterns": [
      "node_modules/"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "quoteProps": "consistent"
  }
}
